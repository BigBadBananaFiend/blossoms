generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./migrations/dev.dv"
}

model User {
    id String @id @default(cuid())
    email String
    password String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    onBoard Boolean @default(false)
}